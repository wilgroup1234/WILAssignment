<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#80704E" />
            <gradient android:angle="-90" android:startColor="#FCBE00" android:endColor="#EBDE7A"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#8A7D6B" />
            <solid android:color="#835648"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#857662" />
            <gradient android:angle="-90" android:startColor="#FCBE00" android:endColor="#EBDE7A" />
        </shape>
    </item>
</selector>
