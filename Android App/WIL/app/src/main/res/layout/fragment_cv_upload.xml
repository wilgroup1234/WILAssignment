<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgb42"
    tools:context=".homeScreen.cvUpload">

    <ImageView
        android:id="@+id/imgCV"
        android:layout_width="122dp"
        android:layout_height="138dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/cvicon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toEndOf="@+id/imageView24"
        app:layout_constraintTop_toBottomOf="@+id/imageView23" />

    <Button
        android:id="@+id/btnELink"
        android:layout_width="218dp"
        android:layout_height="53dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/mybutton"
        android:text="Do Enneagram Test"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgCV" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="283dp"
        android:layout_height="201dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/cvbubble"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="135dp"
        android:layout_height="141dp"
        android:src="@drawable/dog4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView23" />

</android.support.constraint.ConstraintLayout>