<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.a17001922.wil_app.homeScreen.onlineHelpActivity"
    android:background="@drawable/bgb42">

    <TextView
        android:id="@+id/txtOnlineBody"
        android:layout_width="264dp"
        android:layout_height="514dp"
        android:layout_marginTop="44dp"
        android:text="TextView"
        android:textColor="@color/colorEditText"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnOnlineBack"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/mybutton"
        android:text="Back"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtOnlineBody" />

</android.support.constraint.ConstraintLayout>
